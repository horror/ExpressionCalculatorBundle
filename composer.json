{
    "name": "horror/expression-calculator-bundle",
    "description": "Allow you compute some string with simple arithmetic expression",
    "type": "symfony-bundle",
    "homepage": "https://github.com/horror/ExpressionCalculatorBundle",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/framework-bundle": "4.3.*",
        "symfony/yaml": "4.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "horror\\ExpressionCalculatorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "horror\\ExpressionCalculatorBundle\\Tests\\": "tests/"
        }
    }
}
